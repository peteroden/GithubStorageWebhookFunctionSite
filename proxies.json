{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "root": {
            "matchCondition": {
                "route": "/"
            },
            "backendUri": "https://%AZURE_STORAGE_ACCOUNT%.blob.core.windows.net/index.html"
        },
        "index": {
            "matchCondition": {
                "route": "/index.html"
            },
            "backendUri": "https://%AZURE_STORAGE_ACCOUNT%.blob.core.windows.net/index.html"
        },
        "lib": {
            "matchCondition": {
                "route": "/lib/{*restOfPath}"
            },
            "backendUri": "https://%AZURE_STORAGE_ACCOUNT%.blob.core.windows.net/lib/{*restOfPath}"
        }
    }
}